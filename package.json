{
  "name": "faunadb",
  "version": "0.2.3-SNAPSHOT",
  "description": "FaunaDB Javascript driver for Node.JS and Browsers",
  "homepage": "https://fauna.com",
  "repository": "fauna/faunadb-js",
  "license": "MPL-2.0",
  "keywords": [ "database", "fauna", "official", "driver" ],
  "bugs": { "url": "https://github.com/fauna/faunadb-js/issues" },
  "files": [
    "index.d.ts",
    "src/"
  ],
  "main": "index.js",
  "scripts": {
    "doc": "jsdoc -c ./jsdoc.json",
    "test": "mocha test/",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha test/",
    "browserify": "browserify index.js -o dist/faunadb.js",
    "browserify-min": "browserify index.js | uglifyjs -c --screw-ie8 -m -o dist/faunadb-min.js",
    "browser-test-mac": "karma start browserTest.conf.js --browsers Chrome,Safari,Firefox",
    "browser-test-win": "karma start browserTest.conf.js --browsers Chrome,Firefox,IE",
    "browser-test-linux": "karma start browserTest.conf.js --browsers Chrome,Firefox",
    "browser-test-remote": "karma start mobileTest.conf.js"
  },
  "typings": "index.ts.d",
  "dependencies": {
    "base64-js": "^1.2.0",
    "btoa-lite": "^1.0.0",
    "es6-promise": "^3.2.1",
    "fn-annotate": "^1.1.3",
    "object-assign": "^4.1.0",
    "superagent": "^1.8.2"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "ink-docstrap": "^1.2.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "mocha": "^2.4.5",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  }
}
