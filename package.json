{
  "name": "faunadb",
  "version": "0.2.0",
  "description": "FaunaDB (faunadb.com) client for node",
  "homepage": "https://faunadb.com",
  "repository": "faunadb/faunadb-js",
  "license": "MPL-2.0",
  "files": ["src"],
  "typings": "lib",
  "scripts": {
    "install": "npm run build",
    "build": "gulp build-src",
    "watch": "gulp watch-src",
    "build-test": "gulp build-test",
    "watch-build-test": "gulp watch-test",
    "test": "mocha compiled-test/*.js compiled-test/*/*.js",
    "doc": "typedoc src --out doc --name FaunaDB --readme README.md --module commonjs --target ES6 --experimentalAsyncFunctions",
    "lint": "tslint src/*.ts src/model/*.ts test/*.ts test/model/*.ts",
    "all": "npm run build && npm run build-test && npm run test && npm run lint && npm run doc"
  },
  "dependencies": {
    "request": "2.67.0",
    "winston": "1.1.1"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-destructuring": "6.4.0",
    "babel-plugin-transform-es2015-parameters": "6.4.2",
    "babel-plugin-transform-es2015-spread": "6.4.0",
    "babel-plugin-transform-strict-mode": "6.3.13",
    "chai": "3.4.1",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "2.10.0",
    "merge2": "1.0.0",
    "mocha": "2.3.4",
    "tslint": "3.2.2",
    "typedoc": "0.3.12",
    "typescript": "1.7.5"
  },
  "engines": {"node": ">= 4.0.0"}
}
